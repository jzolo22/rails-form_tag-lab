<h1>Student Form</h1>

<%= form_tag students_path do %>
   <p> <label>First Name</label>
    <%= text_field_tag :'student[first_name]' %></p><br>

    <label>Last Name</label>
    <%= text_field_tag :'student[last_name]' %><br>

   <%= submit_tag "Submit Student" %>

<% end%>

<h3><%= Student.first %></h3>